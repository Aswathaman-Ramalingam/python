#Your code below
a=int(input())
b=[i for i in range(2,a+1)]
for i in b:
    if i>1:
        for j in range(2,i):
            if i%j==0:
                break
        else:
            print(i,end=" ")
print()
for i in b:
    if i>1:
        for j in range(2,i):
            if i%j==0:
                print(i,end=" ")
                break

